  "license": "MIT",
  "devDependencies": {
    "chokidar": "^3.6.0",
    "clean-css": "^5.3.3"
  },
  "dependencies": {
    "anymatch": "^3.1.3",
    "binary-extensions": "^2.3.0",
    "braces": "^3.0.3",
    "fill-range": "^7.1.1",
    "glob": "^11.0.2",
    "glob-parent": "^5.1.2",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-webp": "^7.0.0",
    "is-binary-path": "^2.1.0",
    "is-extglob": "^2.1.1",
    "is-glob": "^4.0.3",
    "is-number": "^7.0.0",
    "normalize-path": "^3.0.0",
